<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
</head>
<body class="container">

    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="jumbotron">
            <h1>This is great</h1>
            <p>Welcome to templating using EJS</p>
            <h2>Variables</h2>
            <p><%= tagline %></p>
            <h2>Loop</h2>
            <ul>
                <% drinks.forEach(function(drink) { %>
                    <li><%= drink.agencyName %> - <%= drink.agencyAddress %> </li>
              <%  }); %>
            </ul>
            <h1>Test</h1>

        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>

</body>
</html>
